#include <kipr/wombat.h>
//create a counter pattern that allows you to repeat a loop for a set amount of times
int main()
{
    int count=0;
    
    printf("Repeat the loop 3 times.\n");
    while (count <=2)
    {
            
        motor(0,50);
        motor(3,50);
        msleep(3000);
        
        motor(3,100);
        msleep(1500);
        count = count + 1; 
    }
    
    return 0;
}
